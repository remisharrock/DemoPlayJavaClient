# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                         controllers.Application.index()

GET        /askAwait                 controllers.Application.askAwait()

GET        /askFutures               controllers.Application.askFutures()

GET        /inboxSend                controllers.Application.inboxSend()

GET        /multipleAskAwait         controllers.Application.multipleAskAwait()

GET        /multipleAskFutures         controllers.Application.multipleAskFutures()

GET        /multipleInboxSend        controllers.Application.multipleInboxSend()

GET        /createActor             controllers.Application.createActor()
# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file             controllers.Assets.versioned(path="/public", file: Asset)
